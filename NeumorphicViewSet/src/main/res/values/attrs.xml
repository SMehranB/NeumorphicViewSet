<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <declare-styleable name="NeuButton">

        <attr name="nb_text" format="string" />
        <attr name="nb_HorizontalPadding" format="dimension"/>
        <attr name="nb_VerticalPadding" format="dimension"/>
        <attr name="nb_textSize" format="dimension" />
        <attr name="nb_textColor" format="color"/>
        <attr name="nb_fontFamily" format="reference"/>
        <attr name="nb_textStyle" format="flags">
            <flag name="normal" value="0" />
            <flag name="bold" value="1" />
            <flag name="italic" value="2" />
            <flag name="bold_italic" value="3" />
        </attr>

        <attr name="nb_lightDensity" format="float"/>
        <attr name="nb_shadowDensity" format="float"/>
        <attr name="nb_JutSize" format="enum">
            <enum name="small" value="0"/>
            <enum name="normal" value="1"/>
            <enum name="large" value="2"/>
        </attr>

        <attr name="nb_disabledTextColor" format="color"/>

        <attr name="nb_cornerRadius" format="dimension"/>
        <attr name="nb_backgroundColor" format="color"/>

        <attr name="nb_drawableDimension" format="dimension"/>
        <attr name="nb_drawableStart" format="reference"/>
        <attr name="nb_drawableEnd" format="reference"/>
        <attr name="nb_drawablePadding" format="dimension"/>
        <attr name="nb_drawableTint" format="color"/>

        <attr name="nb_enabled" format="boolean"/>
        
    </declare-styleable>

    <declare-styleable name="NeuImageButton">

        <attr name="nib_HorizontalPadding" format="dimension"/>
        <attr name="nib_VerticalPadding" format="dimension"/>

        <attr name="nib_lightDensity" format="float"/>
        <attr name="nib_shadowDensity" format="float"/>
        <attr name="nib_JutSize" format="enum">
            <enum name="small" value="0"/>
            <enum name="normal" value="1"/>
            <enum name="large" value="2"/>
        </attr>

        <attr name="nib_disabledColor" format="color"/>

        <attr name="nib_cornerRadius" format="dimension"/>
        <attr name="nib_backgroundColor" format="color"/>

        <attr name="nib_drawableDimension" format="dimension"/>
        <attr name="nib_drawable" format="reference"/>
        <attr name="nib_drawableTint" format="color"/>

        <attr name="nib_enabled" format="boolean"/>

    </declare-styleable>

    <declare-styleable name="NeuSeekBar">

        <attr name="nsb_lightDensity" format="float"/>
        <attr name="nsb_shadowDensity" format="float"/>
        <attr name="nsb_JutSize" format="enum">
            <enum name="small" value="0"/>
            <enum name="normal" value="1"/>
            <enum name="large" value="2"/>
        </attr>

        <attr name="nsb_disabledColor" format="color"/>

        <attr name="nsb_backgroundColor" format="color"/>
        <attr name="nsb_handleRadius" format="dimension"/>

        <attr name="nsb_ProgressBarHeight" format="dimension"/>
        <attr name="nsb_ProgressColor" format="color"/>
        <attr name="nsb_Progress" format="integer"/>
        <attr name="nsb_Min" format="integer"/>
        <attr name="nsb_Max" format="integer"/>

        <attr name="nsb_enabled" format="boolean"/>

    </declare-styleable>

    <declare-styleable name="NeuSwitch">

        <attr name="ns_lightDensity" format="float"/>
        <attr name="ns_shadowDensity" format="float"/>
        <attr name="ns_JutSize" format="enum">
            <enum name="small" value="0"/>
            <enum name="normal" value="1"/>
            <enum name="large" value="2"/>
        </attr>

        <attr name="ns_disabledColor" format="color"/>
        <attr name="ns_backgroundColor" format="color"/>
        <attr name="ns_handleColor" format="color"/>

        <attr name="ns_enabled" format="boolean"/>
        <attr name="ns_checked" format="boolean"/>

    </declare-styleable>

    <declare-styleable name="NeuCheckBox">

        <attr name="ncb_text" format="string" />
        <attr name="ncb_HorizontalPadding" format="dimension"/>
        <attr name="ncb_VerticalPadding" format="dimension"/>
        <attr name="ncb_textSize" format="dimension" />
        <attr name="ncb_textColor" format="color"/>
        <attr name="ncb_fontFamily" format="reference"/>
        <attr name="ncb_textStyle" format="flags">
            <flag name="normal" value="0" />
            <flag name="bold" value="1" />
            <flag name="italic" value="2" />
            <flag name="bold_italic" value="3" />
        </attr>

        <attr name="ncb_lightDensity" format="float"/>
        <attr name="ncb_shadowDensity" format="float"/>
        <attr name="ncb_JutSize" format="enum">
            <enum name="small" value="0"/>
            <enum name="normal" value="1"/>
            <enum name="large" value="2"/>
        </attr>

        <attr name="ncb_disabledColor" format="color"/>
        <attr name="ncb_backgroundColor" format="color"/>
        <attr name="ncb_handleColor" format="color"/>

        <attr name="ncb_enabled" format="boolean"/>
        <attr name="ncb_checked" format="boolean"/>

    </declare-styleable>

</resources>